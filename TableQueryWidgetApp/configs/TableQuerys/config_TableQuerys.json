{
  "widgetName": "TableQuerys",
  "inPanelVar": {
    "params": {
      "serviceUrl": "http://sampleserver6.arcgisonline.com/arcgis/rest/services/SampleWorldCities/MapServer/0",
      "tableConfigParams": [
        {
          "header": "City name",
          "fieldName": "CITY_NAME"
        },
        {
          "header": "Population",
          "fieldName": "POP"
        },
        {
          "header": "Rank",
          "fieldName": "POP_RANK"
        },
        {
          "header": "Class",
          "fieldName": "POP_CLASS"
        }
      ],
      "queryConfigParams": [
        {
          "buttonName": "Total population",
          "liParamsArray": [
            {
              "liName": ">500.000",
              "query": "POP<500000"
            },
            {
              "liName": ">1.000.000",
              "query": "POP<1000000 AND POP>500000"
            },
            {
              "liName": ">2.000.000",
              "query": "POP<2000000 AND POP>1000000"
            },
            {
              "liName": "+2.000.000",
              "query": "POP>2000000"
            }
          ]
        },
        {
          "buttonName": "Population rank",
          "liParamsArray": [
            {
              "liName": "RANK 1",
              "query": "POP_RANK = 1"
            },
            {
              "liName": "RANK 2",
              "query": "POP_RANK = 3"
            },
            {
              "liName": "RANK 3",
              "query": "POP_RANK = 2"
            }
          ]
        }
      ]
    }
  }
}